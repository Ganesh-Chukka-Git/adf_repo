{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "ADFChukkaGanesh"
		},
		"BlobStorage_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'BlobStorage'"
		},
		"LS_SQLServer_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'LS_SQLServer'"
		},
		"LS_blobstorage_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'LS_blobstorage'"
		},
		"LS_DeleteBlobFiles_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'LS_DeleteBlobFiles'"
		},
		"LS_AzureKeyVault_properties_typeProperties_baseUrl": {
			"type": "string",
			"defaultValue": "https://keyvaultcgy.vault.azure.net/"
		},
		"LS_SQLServer_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "DESKTOP-8OTJPKL\\CHUKKASQLSERVER"
		},
		"LS_SQLServer_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "DFDB_DEV"
		},
		"LS_SQLServer_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "sa"
		},
		"Daily Chubb Job_properties_ChubDailyLoad_parameters_Container": {
			"type": "string",
			"defaultValue": "containerfiles"
		},
		"Daily Chubb Job_properties_ChubDailyLoad_parameters_Folder": {
			"type": "string",
			"defaultValue": "inputsource/files"
		},
		"Daily Chubb Job_properties_ChubDailyLoad_parameters_FileName": {
			"type": "string",
			"defaultValue": "*.csv"
		},
		"Daily Chubb Job_properties_ChubDailyLoad_parameters_STagingSchemaName": {
			"type": "string",
			"defaultValue": "STG"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "ADFChukkaGanesh_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"BlobStorage_connectionString": {
						"value": "[parameters('BlobStorage_connectionString')]"
					},
					"LS_blobstorage_connectionString": {
						"value": "[parameters('LS_blobstorage_connectionString')]"
					},
					"LS_DeleteBlobFiles_connectionString": {
						"value": "[parameters('LS_DeleteBlobFiles_connectionString')]"
					},
					"LS_SQLServer_password": {
						"value": "[parameters('LS_SQLServer_password')]"
					},
					"LS_AzureKeyVault_properties_typeProperties_baseUrl": {
						"value": "[parameters('LS_AzureKeyVault_properties_typeProperties_baseUrl')]"
					},
					"LS_SQLServer_properties_typeProperties_server": {
						"value": "[parameters('LS_SQLServer_properties_typeProperties_server')]"
					},
					"LS_SQLServer_properties_typeProperties_database": {
						"value": "[parameters('LS_SQLServer_properties_typeProperties_database')]"
					},
					"LS_SQLServer_properties_typeProperties_userName": {
						"value": "[parameters('LS_SQLServer_properties_typeProperties_userName')]"
					},
					"Daily Chubb Job_properties_ChubDailyLoad_parameters_Container": {
						"value": "[parameters('Daily Chubb Job_properties_ChubDailyLoad_parameters_Container')]"
					},
					"Daily Chubb Job_properties_ChubDailyLoad_parameters_Folder": {
						"value": "[parameters('Daily Chubb Job_properties_ChubDailyLoad_parameters_Folder')]"
					},
					"Daily Chubb Job_properties_ChubDailyLoad_parameters_FileName": {
						"value": "[parameters('Daily Chubb Job_properties_ChubDailyLoad_parameters_FileName')]"
					},
					"Daily Chubb Job_properties_ChubDailyLoad_parameters_STagingSchemaName": {
						"value": "[parameters('Daily Chubb Job_properties_ChubDailyLoad_parameters_STagingSchemaName')]"
					}
				}
			},
			"dependsOn": []
		}
	]
