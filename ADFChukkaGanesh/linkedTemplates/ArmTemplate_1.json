{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "ADFChukkaGanesh"
		},
		"Daily Chubb Job_properties_ChubDailyLoad_parameters_Container": {
			"type": "string",
			"defaultValue": "containerfiles"
		},
		"Daily Chubb Job_properties_ChubDailyLoad_parameters_Folder": {
			"type": "string",
			"defaultValue": "inputsource/files"
		},
		"Daily Chubb Job_properties_ChubDailyLoad_parameters_FileName": {
			"type": "string",
			"defaultValue": "*.csv"
		},
		"Daily Chubb Job_properties_ChubDailyLoad_parameters_STagingSchemaName": {
			"type": "string",
			"defaultValue": "STG"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "[concat(parameters('factoryName'), '/Daily Chubb Job')]",
			"type": "Microsoft.DataFactory/factories/triggers",
			"apiVersion": "2018-06-01",
			"properties": {
				"annotations": [],
				"runtimeState": "Started",
				"pipelines": [
					{
						"pipelineReference": {
							"referenceName": "ChubDailyLoad",
							"type": "PipelineReference"
						},
						"parameters": {
							"Container": "[parameters('Daily Chubb Job_properties_ChubDailyLoad_parameters_Container')]",
							"Folder": "[parameters('Daily Chubb Job_properties_ChubDailyLoad_parameters_Folder')]",
							"FileName": "[parameters('Daily Chubb Job_properties_ChubDailyLoad_parameters_FileName')]",
							"STagingSchemaName": "[parameters('Daily Chubb Job_properties_ChubDailyLoad_parameters_STagingSchemaName')]"
						}
					}
				],
				"type": "ScheduleTrigger",
				"typeProperties": {
					"recurrence": {
						"frequency": "Minute",
						"interval": 15,
						"startTime": "2025-11-19T16:25:00",
						"endTime": "2025-11-19T18:00:00",
						"timeZone": "India Standard Time"
					}
				}
			},
			"dependsOn": []
		},
		{
			"name": "[concat(parameters('factoryName'), '/Daily Job Chub')]",
			"type": "Microsoft.DataFactory/factories/triggers",
			"apiVersion": "2018-06-01",
			"properties": {
				"annotations": [],
				"runtimeState": "Started",
				"pipelines": [
					{
						"pipelineReference": {
							"referenceName": "ChubDailyLoad",
							"type": "PipelineReference"
						},
						"parameters": {}
					}
				],
				"type": "ScheduleTrigger",
				"typeProperties": {
					"recurrence": {
						"frequency": "Minute",
						"interval": 5,
						"startTime": "2025-11-19T16:18:00",
						"endTime": "2025-11-30T10:39:00",
						"timeZone": "India Standard Time"
					}
				}
			},
			"dependsOn": []
		}
	]
}